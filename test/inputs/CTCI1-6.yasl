fn compress(s) {
    new_s := ''
    i := 0
    while i < len s {
        count := 0
        j := i
        while i < len s && s->__get(i) == s->__get(j) {
            count += 1
            i += 1
        }
        new_s ~= s->__get(j) ~ count
    }
    if len s <= len new_s {
        return s
    }
    return new_s
}


echo compress('aabccccaaa')
echo compress('abcde')

